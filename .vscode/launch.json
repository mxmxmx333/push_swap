	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/a.out",
			"args": ["33", "65", "99", "51", "85", "32", "35", "73", "55", "16", "31" "96" "67" "53" "78" "72" "38" "77" "63" "87" 18 3 21 97 94 2 89 81 91 56 83 98 92 27 60 50 62 58 49 88 11 59 79 46 17 76 82 52 48 44 40 45 39 13 28 84 36 6 19 64 9 34 95 69 66 25 1 90 42 86 68 14 75 47 4 30 23 29 74 80 24 70 15 61 37 12 93 20 26 100 5 41 43 8 10 22 7 57 71 54 ],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		}

	]
